    <EmbeddedResource Remove="IB\TreeGridView\Samples\**" />
    <None Remove="FormsDesignerBinding\Dependecies\**" />
    <None Remove="IB\TreeGridView\Samples\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="IB\Refactoring\Bookmarks.cs" />
